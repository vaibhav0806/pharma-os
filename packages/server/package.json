{
  "name": "@pharma-os/server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "migrate": "dotenv -e ../../.env -- node-pg-migrate up -m src/db/migrations",
    "migrate:down": "dotenv -e ../../.env -- node-pg-migrate down -m src/db/migrations",
    "migrate:create": "node-pg-migrate create -m src/db/migrations"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "twilio": "^4.20.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "dotenv-cli": "^11.0.0",
    "node-pg-migrate": "^6.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
